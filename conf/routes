GET			/publication/allPublications 							controllers.PublicationController.showAllPublications()
GET			/publication/allPublicationTopics						controllers.PublicationController.showAllPublicationTopics()
GET			/publication/allPublicationTopicKeywords				controllers.PublicationController.showAllPublicationTopicKeywords()
GET			/publication/overview 									controllers.PublicationController.showOverview()

GET 		/publication/publicationMetadata/:publicationId         controllers.PublicationController.showPublicationMetadata(publicationId: Integer)
GET 		/publication/publicationPanel/:publicationId         	controllers.PublicationController.showPublicationPanel(publicationId: Integer)
GET 		/publication/publicationsByTopicId/:topicId         	controllers.PublicationController.showPublicationsByTopicId(topicId: Integer)


GET		    /climateService/allPublications							controllers.PublicationController.showAllPublications()
GET		    /climateService/mostRecentlyUsedPublications			controllers.PublicationController.mostRecentlyUsedPublications()
GET         /climateService/mostRecentlyAddedPublications			controllers.PublicationController.mostRecentlyAddedPublications()
GET		    /publication/searchPublication 					 		controllers.PublicationController.searchPublications()
GET 	    /publication/getSearchResult				    		controllers.PublicationController.getSearchResult()

